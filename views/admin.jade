extends layout

block content
	.container
		h3= title
		.row
			.span6
				#admins
					table(class="table")
						thead
							tr
								td #
								td email
								td password
								td
						tbody
							- each admin, i in admins
								tr
									td #{i+1}
									td #{admin.email}
									td #{admin.password}
									td 
										a(href="admin/view/" + admin._id.toHexString()) view