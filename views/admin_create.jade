					label( class = "control-label" ) Email
					.controls
						input( type = "text", name = "txt_email", placeholder = "admin@hours.vn" )
				.control-group
					label( class = "control-label" ) Password
					.controls
						input( type = "password", name = "txt_password" )
				.control-group
					label(class = "control-label" ) 
